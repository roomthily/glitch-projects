<div class="content">
  <div class="highlights">
    {{#each projects.highlights}}
      {{> highlight-project project_name=this.domain project_description=this.decription project_id=this.id}}
    {{/each}}
  </div>
  <div class="projects">
    {{#each projects.projects}}
      {{! load the partial template for the project }}
      {{> project project_name=this.domain project_description=this.decription project_id=this.id}}
    {{/each}}
  </div>
</div>